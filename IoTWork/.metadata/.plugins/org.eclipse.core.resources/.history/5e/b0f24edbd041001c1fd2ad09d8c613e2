package basic;

import java.net.InetAddress;
import java.net.UnknownHostException;

//InetAddress의 테스트 - IP
public class InetAddressTest {
	public static void main(String[] args) {
		try {
			InetAddress ia = InetAddress.getByName(args[0]);
		} catch (UnknownHostException e) {
			e.printStackTrace();
		}
		
	}
}
